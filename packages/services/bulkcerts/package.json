{
  "name": "@cdf/bulkcerts",
  "version": "3.1.2",
  "description": "CDF Bulk Certificate Creation",
  "author": "Dean Hart <deanhart@amazon.com>",
  "config": {
    "environment": "development"
  },
  "scripts": {
    "reset": "npm run clean && del-cli node_modules",
    "clean": "del-cli dist build",
    "lint": "tslint \"src/**/*.ts\"",
    "build": "tsc -b tsconfig.json",
    "test": "npm run lint && NODE_ENV=local_test NODE_CONFIG_DIR='./src/config' nyc jest",
    "start": "npm run build && NODE_CONFIG=$(cat ${CONFIG_LOCATION}/commands/${npm_package_config_environment}-config.json) NODE_CONFIG_DIR='./src/config' concurrently \"npm run build -- -w\" \"nodemon dist/lambda_proxy.js\"",
    "prebundle:prepare": "cpy src/config/* dist/config/ && make-dir build && make-dir dist/node_modules",
    "bundle:package": "cd dist && zip -rq ../build/build.zip * && zip -urq ../build/build.zip ../package.json && zip -urq ../build/build.zip ../node_modules",
    "prebundle:quick": "pnpm i --force && pnpm run build && pnpm run prebundle:prepare",
    "bundle:quick": "pnpm i --no-lock --prod --shamefully-flatten --force && pnpm run bundle:package",
    "postbundle:quick": "pnpm i --force",
    "semantic-release": "npx semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "@cdf/config-inject": "^2.0.0",
    "@cdf/express-middleware": "^1.0.0",
    "aws-sdk": "2.418.0",
    "aws-serverless-express": "3.3.5",
    "body-parser": "1.18.3",
    "config": "3.0.1",
    "cors": "^2.8.5",
    "express": "4.16.4",
    "inversify": "5.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "js-yaml": "3.12.2",
    "jszip": "3.2.0",
    "ow": "0.12.0",
    "pem": "1.14.2",
    "reflect-metadata": "0.1.13",
    "uuid": "3.3.2",
    "winston": "2.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/npm": "5.1.4",
    "@semantic-release/release-notes-generator": "7.1.4",
    "@types/aws-serverless-express": "3.3.0",
    "@types/body-parser": "1.17.0",
    "@types/config": "0.0.34",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.0",
    "@types/jszip": "^3.1.5",
    "@types/multer": "1.3.7",
    "@types/node": "8.10.43",
    "@types/pem": "1.9.5",
    "@types/uuid": "3.4.4",
    "commitizen": "3.0.7",
    "concurrently": "4.1.0",
    "cpy-cli": "2.0.0",
    "del-cli": "1.1.0",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "24.0.0",
    "jest-mock": "24.0.0",
    "jest-resolve": "24.5.0",
    "make-dir-cli": "1.0.0",
    "nodemon": "1.18.10",
    "nyc": "13.3.0",
    "semantic-release": "15.13.3",
    "semantic-release-monorepo": "6.1.1",
    "ts-jest": "24.0.0",
    "tslint": "5.13.1",
    "typescript": "3.1.6"
  },
  "jest": {
    "globals": {
      "NODE_CONFIG_DIR": "./src/config"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": true
}
