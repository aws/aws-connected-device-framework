{
  "name": "@cdf/assetlibrary",
  "version": "4.0.3",
  "description": "CDF Asset Library",
  "author": "Dean Hart <deanhart@amazon.com>",
  "config": {
    "environment": "development"
  },
  "scripts": {
    "clean": "npx shx rm -rf dist deploy tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b && npx shx cp -R 'src/types/definitions' 'dist/types/definitions' && npx shx cp -R 'src/config' 'dist/'",
    "test": "rushx lint && NODE_ENV= NODE_CONFIG_DIR='./src/config' jest --silent",
    "start": "rushx build && NODE_CONFIG=$(cat ${CONFIG_LOCATION}/assetlibrary/${npm_package_config_environment}-config.json) NODE_CONFIG_DIR='./src/config' nodemon dist/lambda_proxy.js",
    "prebundle": "npx shx rm -rf deploy bundle.zip && npx shx mkdir deploy",
    "bundle": "rush deploy --overwrite --project ${npm_package_name} --target-folder deploy",
    "postbundle": "cd deploy && zip --symlinks -rq ../bundle.zip *"
  },
  "dependencies": {
    "@cdf/errors": "^3.0.0",
    "@cdf/logger": "^3.0.0",
    "@cdf/config-inject": "^3.0.0",
    "@cdf/express-middleware": "^2.0.0",
    "ajv": "6.10.2",
    "async-local-storage": "2.3.1",
    "aws-sdk": "2.804.0",
    "body-parser": "1.19.0",
    "config": "3.3.6",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-version-request": "1.7.0",
    "fast-json-patch": "3.0.0-1",
    "gremlin": "3.4.8",
    "inversify": "5.1.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "js-yaml": "4.1.0",
    "json-stable-stringify": "1.0.1",
    "jsonwebtoken": "8.5.1",
    "logform": "2.2.0",
    "mime": "2.5.2",
    "node-cache": "5.1.2",
    "ow": "0.23.0",
    "prettyjson": "1.2.1",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "serverless-http": "2.7.0",
    "winston": "3.3.3",
    "winston-transport": "4.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "2.3.4",
    "@types/body-parser": "1.19.0",
    "@types/config": "0.0.38",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/express-serve-static-core": "4.17.19",
    "@types/express-version-request": "1.7.1",
    "@types/jest": "26.0.23",
    "@types/json-stable-stringify": "1.0.32",
    "@types/jsonwebtoken": "8.5.0",
    "@types/prettyjson": "0.0.29",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "7.26.0",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-express": "1.12.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": true
}
