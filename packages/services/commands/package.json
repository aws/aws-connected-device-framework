{
  "name": "@cdf/commands",
  "version": "4.1.0",
  "description": "CDF Commands",
  "author": "Dean Hart <deanhart@amazon.com>",
  "config": {
    "environment": "development"
  },
  "scripts": {
    "clean": "npx shx rm -rf dist deploy tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b && npx shx cp -R 'src/config' 'dist/'",
    "test": "rushx lint && NODE_ENV= NODE_CONFIG_DIR='./src/config' jest --silent",
    "start": "rushx build && NODE_CONFIG=$(cat ${CONFIG_LOCATION}/commands/${npm_package_config_environment}-config.json) NODE_CONFIG_DIR='./src/config' nodemon dist/lambda_proxy.js",
    "prebundle": "npx shx rm -rf deploy bundle.zip && npx shx mkdir deploy",
    "bundle": "rush deploy --overwrite --project ${npm_package_name} --target-folder deploy",
    "postbundle": "cd deploy && zip --symlinks -rq ../bundle.zip *"
  },
  "dependencies": {
    "@cdf/errors": "^3.0.0",
    "@cdf/logger": "^3.0.0",
    "@cdf/lambda-invoke": "^2.0.0",
    "@cdf/assetlibrary-client": "^4.0.1",
    "@cdf/express-middleware": "^2.0.0",
    "@cdf/config-inject": "^3.0.0",
    "@cdf/provisioning-client": "^4.0.1",
    "ajv": "6.10.2",
    "aws-sdk": "2.777.0",
    "body-parser": "1.19.0",
    "busboy": "0.3.1",
    "config": "3.2.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "inversify": "5.0.5",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "js-yaml": "3.13.1",
    "logform": "2.1.2",
    "ow": "0.23.0",
    "readable-stream": "2.3.6",
    "reflect-metadata": "0.1.13",
    "serverless-http": "2.6.0",
    "sinon": "9.2.1",
    "superagent": "6.1.0",
    "uuid": "8.3.2",
    "winston": "3.2.1",
    "winston-transport": "4.3.0",
    "supports-color": "~8.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "2.3.1",
    "@types/body-parser": "1.19.0",
    "@types/busboy": "0.2.3",
    "@types/config": "0.0.37",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.9",
    "@types/jest": "26.0.23",
    "@types/sinon": "9.0.9",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "eslint": "7.15.0",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "nodemon": "2.0.7",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "jest": {
    "globals": {
      "NODE_CONFIG_DIR": "./src/config"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": true
}
