{
  "name": "@cdf/assetlibrary",
  "version": "1.3.0",
  "description": "CDF Asset Library",
  "author": "Dean Hart <deanhart@amazon.com>",
  "main": "dist/app.js",
  "scripts": {
    "clean": "del-cli dist build",
    "build": "tsc -b tsconfig.json",
    "prebundle": "cpy package.json dist && cpy src/types/definitions/* dist/types/definitions/ && cpy src/config/* dist/config/ && del-cli ../../../cdf-monorepo-builds/assetlibrary/dist --force && cpy dist ../../../cdf-monorepo-builds/assetlibrary/ --parents && del-cli ../../../cdf-monorepo-builds/assetlibrary/dist/node_modules --force && pnpm i --prod --shamefully-flatten --force && cp -LR node_modules ../../../cdf-monorepo-builds/assetlibrary/dist/",
    "bundle": "cd ../../../cdf-monorepo-builds/assetlibrary/dist && zip -rq ../build.zip *",
    "postbundle": "echo '\n\n******   node_modules have been resolved in production mode.  If this is a dev environment, make sure you run `pnpm i --force` to reset.'",
    "start": "NODE_CONFIG=$(cat ${ASSETLIBRARY_CONFIG_LOCATION}) NODE_CONFIG_DIR='./src/config' ts-node src/lambda_proxy.ts"
  },
  "dependencies": {
    "@cdf/config-inject": "^1.2.0",
    "ajv": "^6.8.1",
    "aws-sdk": "^2.398.0",
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "express": "^4.16.4",
    "fast-json-patch": "^2.0.7",
    "flatted": "^2.0.0",
    "gremlin": "3.2.9",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "^6.3.2",
    "js-yaml": "^3.12.1",
    "node-cache": "^4.2.0",
    "ow": "^0.12.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3.3.0",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.1",
    "@types/jest": "~24.0.0",
    "@types/node": "~8.10.40",
    "aws-sdk-mock": "^4.3.0",
    "cpy-cli": "^2.0.0",
    "del-cli": "^1.1.0",
    "jest": "~24.1.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-mock": "^24.0.0",
    "make-dir-cli": "^1.0.0",
    "nyc": "^13.1.0",
    "ts-jest": "24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "jest": {
    "globals": {
      "NODE_CONFIG_STRICT_MODE": false,
      "SUPPRESS_NO_CONFIG_WARNING": true
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
