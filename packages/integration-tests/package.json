{
  "name": "@cdf/integration-tests",
  "version": "1.0.2",
  "description": "CDF Integration Tests",
  "author": "Dean Hart <deanhart@amazon.com>",
  "scripts": {
    "reset": "npm run clean && del-cli node_modules",
    "clean": "del-cli .nyc_output coverage dist build features/step_definitions features/support",
    "lint": "tslint \"src/**/*.ts\"",
    "build": "tsc -b tsconfig.json && cpy dist/step_definitions/**/* features/step_definitions/ --parents && cpy src/testResources/**/* dist/testResources/ --parents && cpy dist/support/**/* features/support/ --parents",
    "integration-test": "NODE_CONFIG=$(cat ${INTEGRATIONTESTS_CONFIG_LOCATION}) NODE_CONFIG_DIR='./src/config' cucumber-js",
    "semantic-release": "npx semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "@cdf/assetlibrary-client": "^1.0.0",
    "@cdf/assetlibraryhistory-client": "^1.0.0",
    "@cdf/commands-client": "^1.0.0",
    "@cdf/provisioning-client": "^1.0.0",
    "aws-sdk": "^2.398.0",
    "chai": "^4.1.2",
    "chai-string": "^1.4.0",
    "config": "^1.31.0",
    "cucumber": "^4.2.1",
    "js-yaml": "^3.12.0",
    "json-stable-stringify": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "typed-rest-client": "^1.0.7",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/chai": "^4.1.4",
    "@types/chai-string": "^1.4.1",
    "@types/config": "0.0.34",
    "@types/cucumber": "^4.0.4",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/node": "~8.10.40",
    "commitizen": "^3.0.7",
    "concurrently": "^4.1.0",
    "cpy-cli": "^1.0.1",
    "del-cli": "^0.2.1",
    "husky": "^1.3.1",
    "make-dir-cli": "^1.0.0",
    "nodemon": "^1.18.10",
    "semantic-release": "^15.13.3",
    "semantic-release-monorepo": "^6.1.1",
    "tslint": "^5.12.1",
    "typescript": "3.1.6"
  }
}
