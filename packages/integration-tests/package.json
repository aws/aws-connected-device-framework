{
  "name": "@cdf/integration-tests",
  "version": "2.0.0",
  "description": "CDF Integration Tests",
  "author": "Dean Hart <deanhart@amazon.com>",
  "config": {
    "environment": "development"
  },
  "scripts": {
    "reset": "npm run clean && del-cli node_modules",
    "clean": "del-cli .nyc_output coverage dist build features/step_definitions features/support",
    "lint": "tslint \"src/**/*.ts\"",
    "build": "tsc -b tsconfig.json && cpy dist/step_definitions/**/* features/step_definitions/ --parents && cpy src/testResources/**/* dist/testResources/ --parents && cpy dist/support/**/* features/support/ --parents",
    "integration-test": "NODE_CONFIG=$(cat ${CONFIG_LOCATION}/integration-tests/${npm_package_config_environment}-config.json) NODE_CONFIG_DIR='./src/config' cucumber-js",
    "semantic-release": "npx semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "@cdf/assetlibrary-client": "^3.0.0",
    "@cdf/assetlibraryhistory-client": "^2.0.0",
    "@cdf/commands-client": "^2.0.0",
    "@cdf/provisioning-client": "^3.0.0",
    "aws-sdk": "2.418.0",
    "chai": "4.2.0",
    "chai-string": "1.5.0",
    "config": "3.0.1",
    "cucumber": "4.2.1",
    "js-yaml": "3.12.2",
    "json-stable-stringify": "1.0.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "0.1.13",
    "typed-rest-client": "1.1.2",
    "winston": "2.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/npm": "5.1.4",
    "@semantic-release/release-notes-generator": "7.1.4",
    "@types/chai": "4.1.7",
    "@types/chai-string": "1.4.1",
    "@types/config": "0.0.34",
    "@types/cucumber": "4.0.5",
    "@types/json-stable-stringify": "1.0.32",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "8.10.43",
    "commitizen": "3.0.7",
    "concurrently": "4.1.0",
    "cpy-cli": "2.0.0",
    "del-cli": "3.0.0",
    "husky": "1.3.1",
    "make-dir-cli": "2.0.0",
    "nodemon": "1.18.10",
    "semantic-release": "15.13.3",
    "semantic-release-monorepo": "6.1.1",
    "tslint": "5.13.1",
    "typescript": "3.1.6"
  }
}
