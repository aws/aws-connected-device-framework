{
  "name": "@cdf/integration-tests",
  "version": "2.7.0",
  "description": "CDF Integration Tests",
  "author": "Dean Hart <deanhart@amazon.com>",
  "config": {
    "environment": "development"
  },
  "scripts": {
    "clean": "npx shx rm -rf dist build tsconfig.tsbuildinfo .nyc_output coverage dist build features/dist",
    "lint": "npx eslint . --ext '.ts'",
    "prebuild": "rushx clean",
    "build": "npx tsc && npx shx mkdir -p features/dist && npx shx cp -R 'dist/step_definitions' 'features/dist/step_definitions' && npx shx cp -R 'src/testResources' 'dist/testResources' && npx shx cp -R 'dist/support' 'features/dist/support' && npx shx cp -R 'dist/di' 'features/dist/di'",
    "integration-test": "NODE_CONFIG=$(cat ${CONFIG_LOCATION}/integration-tests/${npm_package_config_environment}-config.json) NODE_CONFIG_DIR='./src/config' cucumber-js"
  },
  "dependencies": {
    "@cdf/assetlibrary-client": "^4.0.1",
    "@cdf/assetlibraryhistory-client": "^4.0.1",
    "@cdf/commands-client": "^4.1.0",
    "@cdf/errors": "^3.0.0",
    "@cdf/lambda-invoke": "^2.0.0",
    "@cdf/logger": "^3.0.0",
    "@cdf/notifications-client": "^3.0.1",
    "@cdf/provisioning-client": "^4.0.1",
    "@cdf/greengrass-deployment-client": "^1.0.0",
    "@cdf/greengrass-provisioning-client": "^2.1.0",
    "aws-sdk": "2.804.0",
    "aws-iot-device-sdk": "2.2.6",
    "chai": "4.2.0",
    "chai-string": "1.5.0",
    "chai-uuid": "1.0.6",
    "config": "3.3.6",
    "cucumber": "6.0.5",
    "inversify": "5.1.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "js-yaml": "4.1.0",
    "json-stable-stringify": "1.0.1",
    "jsonwebtoken": "8.5.1",
    "logform": "2.2.0",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "superagent": "6.1.0",
    "typed-rest-client": "1.8.0",
    "winston": "3.3.3",
    "winston-transport": "4.4.0",
    "jsonpath-plus": "~5.0.7"
  },
  "devDependencies": {
    "@types/aws-iot-device-sdk": "2.2.2",

    "@types/chai": "4.2.14",
    "@types/chai-string": "1.4.2",
    "@types/chai-uuid": "^1.0.2",
    "@types/config": "0.0.38",
    "@types/cucumber": "6.0.1",
    "@types/json-stable-stringify": "1.0.32",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.6.1",
    
    "shx": "0.3.3",
    "typescript": "4.2.4",
    "@types/jsonpath-plus": "~5.0.1"
  },
  "private": true
}
