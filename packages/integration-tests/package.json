{
  "name": "@cdf/integration-tests",
  "version": "2.1.0",
  "description": "CDF Integration Tests",
  "author": "Dean Hart <deanhart@amazon.com>",
  "config": {
    "environment": "development"
  },
  "scripts": {
    "clean": "rm -rf dist build tsconfig.tsbuildinfo .nyc_output coverage dist build features/step_definitions features/support",
    "reset": "rm -rf cli node_modules package-lock.json",
    "lint": "tslint \"src/**/*.ts\"",
    "build": "tsc -b tsconfig.json && cpy dist/step_definitions/**/* features/step_definitions/ --parents && cpy src/testResources/**/* dist/testResources/ --parents && cpy dist/support/**/* features/support/ --parents",
    "integration-test": "NODE_CONFIG=$(cat ${CONFIG_LOCATION}/integration-tests/${npm_package_config_environment}-config.json) NODE_CONFIG_DIR='./src/config' cucumber-js",
    "semantic-release": "npx semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "@cdf/assetlibrary-client": "^3.0.0",
    "@cdf/assetlibraryhistory-client": "^3.0.0",
    "@cdf/commands-client": "^3.0.0",
    "@cdf/provisioning-client": "^3.0.0",
    "aws-sdk": "2.588.0",
    "chai": "4.2.0",
    "chai-string": "1.5.0",
    "config": "3.2.4",
    "cucumber": "6.0.5",
    "js-yaml": "3.13.1",
    "json-stable-stringify": "1.0.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "0.1.13",
    "typed-rest-client": "1.5.0",
    "readable-stream": "2.3.6",
    "winston": "3.2.1",
    "winston-transport": "4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/release-notes-generator": "7.3.5",
    "@types/chai": "4.2.7",
    "@types/chai-string": "1.4.2",
    "@types/config": "0.0.36",
    "@types/cucumber": "6.0.0",
    "@types/json-stable-stringify": "1.0.32",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "12.12.17",
    "commitizen": "4.0.3",
    "concurrently": "5.0.1",
    "cpy-cli": "3.0.0",
    "del-cli": "3.0.0",
    "husky": "3.1.0",
    "make-dir-cli": "2.0.0",
    "nodemon": "2.0.2",
    "semantic-release": "15.13.31",
    "semantic-release-monorepo": "6.1.1",
    "tslint": "5.20.1",
    "typescript": "3.7.3"
  }
}
