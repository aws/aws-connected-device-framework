{
  "name": "@cdf/errors",
  "version": "1.2.0",
  "description": "Library for CDF base errors",
  "author": "Atul Bargaje <atulbar@amazon.com>",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "del-cli dist",
    "build": "tsc -b tsconfig.json"
  },
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/node": "~8.10.40",
    "del-cli": "^1.1.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "globals": {
      "NODE_CONFIG_STRICT_MODE": false,
      "SUPPRESS_NO_CONFIG_WARNING": true
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC",
  "bundledDependencies": []
}