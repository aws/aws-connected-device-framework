{
  "name": "@cdf/config-inject",
  "version": "1.2.0",
  "description": "node-config variables are injected into inversify IoC container. If you are using inversify with node-config this library will be useful. Loosly based on inversify-config-injection library.",
  "author": "Atul Bargaje <atulbar@amazon.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "reset": "pnpm run clean && del-cli node_modules",
    "clean": "del-cli dist build",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "pnpm run lint && nyc jest",
    "build": "tsc -b tsconfig.json"
  },
  "dependencies": {
    "@cdf/errors": "^1.2.0",
    "@cdf/logger": "^1.2.0",
    "config": "^3.0.1",
    "js-yaml": "^3.12.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.4",
    "@types/config": "0.0.34",
    "@types/jest": "~24.0.0",
    "@types/node": "~8.10.40",
    "commitizen": "^3.0.7",
    "del-cli": "^1.1.0",
    "jest": "~24.1.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-mock": "^24.0.0",
    "nyc": "^13.1.0",
    "semantic-release": "^15.13.3",
    "semantic-release-monorepo": "^6.1.1",
    "ts-jest": "24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "jest": {
    "globals": {
      "NODE_CONFIG_STRICT_MODE": false,
      "SUPPRESS_NO_CONFIG_WARNING": true
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC",
  "private": true,
  "release": {
    "monorepo": {
      "analyzeCommits": {
        "format": "atom"
      },
      "generateNotes": "myNotesGenerator"
    },
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "verifyConditions": [
      "@semantic-release/git"
    ]
  }
}
