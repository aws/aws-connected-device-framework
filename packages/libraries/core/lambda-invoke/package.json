{
  "name": "@cdf/lambda-invoke",
  "version": "2.0.0",
  "description": "Lambda Invoker Service",
  "author": "Hassan Khokhar <hkhokhar@amazon.com>",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "npx shx rm -rf dist deploy tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b",
    "test": "rushx lint && NODE_ENV= NODE_CONFIG_DIR='./src/config' jest --silent --passWithNoTests"
  },
  "dependencies": {
    "@cdf/config-inject": "^3.0.0",
    "config": "3.2.4",
    "http-errors": "1.8.0",
    "inversify": "5.0.5",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "js-yaml": "3.13.1",
    "logform": "2.1.2",
    "reflect-metadata": "0.1.13",
    "superagent": "6.1.0",
    "readable-stream": "2.3.6",
    "winston": "3.2.1",
    "winston-transport": "4.3.0"
  },
  "peerDependencies": {
    "aws-sdk": "2.777.0"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "2.3.1",
    "eslint": "7.15.0",

    "nyc": "15.1.0",
    "shx": "0.3.3",

    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-mock": "26.6.2",
    "jest-resolve": "26.6.2",
    "jest": "26.6.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",

    "@types/config": "0.0.37",
    "@types/http-errors": "1.8.0"
  },
  "jest": {
    "globals": {
      "NODE_CONFIG_STRICT_MODE": false,
      "SUPPRESS_NO_CONFIG_WARNING": true
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC",
  "private": true
}
