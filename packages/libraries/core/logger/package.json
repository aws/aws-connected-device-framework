{
  "name": "@cdf/logger",
  "description": "Common logging capability for platform.",
  "version": "2.0.0",
  "author": "Atul Bargaje <atulbar@amazon.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "reset": "npm run clean && del-cli node_modules",
    "clean": "del-cli dist build",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "npm run lint && nyc jest --passWithNoTests",
    "build": "tsc -b tsconfig.json",
    "semantic-release": "npx semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "winston": "2.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/npm": "5.1.4",
    "@semantic-release/release-notes-generator": "7.1.4",
    "@types/jest": "24.0.11",
    "@types/node": "8.10.43",
    "commitizen": "3.0.7",
    "concurrently": "4.1.0",
    "del-cli": "3.0.0",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "jest-haste-map": "24.0.0",
    "jest-mock": "24.0.0",
    "jest-resolve": "24.5.0",
    "nodemon": "1.18.10",
    "nyc": "13.3.0",
    "semantic-release": "15.13.3",
    "semantic-release-monorepo": "6.1.1",
    "ts-jest": "24.0.0",
    "tslint": "5.13.1",
    "typescript": "3.1.6"
  },
  "jest": {
    "globals": {
      "NODE_CONFIG_STRICT_MODE": false,
      "SUPPRESS_NO_CONFIG_WARNING": true
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC",
  "private": true
}
