{
  "name": "@cdf/assetlibrary-export",
  "version": "3.1.0",
  "description": "CDF Asset Library Export Service",
  "author": "Hassan Khokhar",
  "scripts": {
    "clean": "rushx clean:postrelease && npx shx rm -f bundle.zip",
    "clean:postrelease": "npx shx rm -rf dist deploy tsconfig.tsbuildinfo .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b && npx shx cp -R 'src/config' 'dist/config'",
    "test": "rushx lint && jest --silent",
    "start": "rushx build && APP_CONFIG_DIR=${PWD}/src/config nodemon dist/lambda_proxy.js",
    "bundle": "../../../infrastructure/bundle-package.bash assetlibrary-export"
  },
  "dependencies": {
    "@cdf/logger": "^4.0.0",
    "@cdf/config-inject": "^4.0.1",
    "aws-sdk": "2.1048.0",
    "gremlin": "3.4.8",
    "inversify": "5.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "js-yaml": "3.13.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "logform": "2.1.2",
    "mime": "1.6.0",
    "moment": "2.29.1",
    "luxon": "2.3.0",
    "ow": "0.15.0",
    "p-limit": "3.1.0",
    "readable-stream": "2.3.6",
    "reflect-metadata": "0.1.13",
    "serverless-http": "2.3.1",
    "shortid": "2.2.16",
    "winston": "3.2.1",
    "winston-transport": "4.3.0",
    "dotenv-flow": "~3.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "2.3.1",
    "@types/body-parser": "1.19.0",
    "@types/cors": "2.8.8",
    "@types/express": "4.17.9",
    "@types/express-serve-static-core": "4.17.16",
    "@types/express-version-request": "1.7.1",
    "@types/jest": "26.0.18",
    "@types/json-stable-stringify": "1.0.32",
    "@types/jsonwebtoken": "8.5.0",
    "@types/luxon": "2.0.9",
    "@types/lodash":"4.14.168",
    "@types/prettyjson": "0.0.29",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "eslint": "7.15.0",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-express": "1.12.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.10",
    "jest-resolve": "26.6.2",
    "nodemon": "2.0.6",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "ts-jest": "26.4.4",
    "typescript": "4.2.4",
    "@types/dotenv-flow": "~3.2.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": true
}
