{
  "name": "@aws-solutions/cdf-commands",
  "version": "0.0.0",
  "description": "CDF Commands",
  "author": "Dean Hart",
  "scripts": {
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b && npx shx cp -R 'src/config' 'dist/'",
    "test": "rushx lint && NODE_ENV= jest --silent --maxWorkers=$JEST_MAX_WORKERS",
    "start": "rushx build && APP_CONFIG_DIR=${PWD}/src/config nodemon dist/lambda_proxy.js",
    "bundle": "../../../infrastructure/bundle-package.bash commands"
  },
  "dependencies": {
    "@aws-solutions/cdf-errors": "^0.0.0",
    "@aws-solutions/cdf-logger": "^0.0.0",
    "@aws-solutions/cdf-lambda-invoke": "^0.0.2",
    "@aws-solutions/cdf-assetlibrary-client": "^0.0.2",
    "@aws-solutions/cdf-express-middleware": "^0.0.0",
    "@aws-solutions/cdf-config-inject": "^0.0.0",
    "@aws-solutions/cdf-provisioning-client": "^0.0.2",
    "ajv": "6.12.3",
    "aws-sdk": "2.1048.0",
    "body-parser": "1.20.1",
    "busboy": "0.3.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.4.3",
    "js-yaml": "4.1.0",
    "logform": "2.4.2",
    "ow": "0.23.0",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "serverless-http": "2.7.0",
    "sinon": "9.2.1",
    "superagent": "7.1.1",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "winston-transport": "4.4.0",
    "supports-color": "~8.0.0",
    "dotenv-flow": "~3.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "2.3.4",
    "@types/body-parser": "1.19.2",
    "@types/busboy": "0.2.3",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.16",
    "@types/jest": "26.0.23",
    "@types/sinon": "9.0.9",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "7.26.0",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "nodemon": "2.0.20",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "@types/dotenv-flow": "~3.2.0"
  },
  "jest": {
    "globals": {
      "NODE_CONFIG_DIR": "./src/config"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": true
}
