{
  "name": "@awssolutions/cdf-greengrass2-provisioning",
  "version": "0.0.0",
  "description": "CDF Greengrass V2 Provisioning",
  "author": "Dean Hart",
  "files": [
    "bundle.zip"
  ],
  "scripts": {
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b && npx shx cp -R 'src/config' 'dist/'",
    "test": "rushx lint && NODE_ENV= jest --silent --passWithNoTests --maxWorkers=$JEST_MAX_WORKERS",
    "start": "rushx build && APP_CONFIG_DIR=${PWD}/src/config node dist/lambda_apigw_proxy.js",
    "bundle": "npm run bundle:build && npm run bundle:deploy",
    "bundle:build": "lambda-zip --entry-point src/lambda_*.ts --copy config:src/config",
    "bundle:deploy": "node ../../../infrastructure/bundle-package.js"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "3.38.0",
    "@aws-sdk/client-dynamodb": "3.38.0",
    "@aws-sdk/client-eventbridge": "3.38.0",
    "@aws-sdk/client-greengrassv2": "3.38.0",
    "@aws-sdk/client-iot": "3.38.0",
    "@aws-sdk/client-lambda": "3.38.0",
    "@aws-sdk/client-s3": "3.38.0",
    "@aws-sdk/client-sqs": "3.38.0",
    "@aws-sdk/lib-dynamodb": "3.38.0",
    "@aws-sdk/lib-storage": "3.38.0",
    "@aws-sdk/signature-v4-crt": "3.38.0",
    "@aws-sdk/smithy-client": "3.38.0",
    "@aws-sdk/types": "3.38.0",
    "@aws-sdk/util-dynamodb": "3.38.0",
    "@awssolutions/cdf-assetlibrary-client": "^0.0.0",
    "@awssolutions/cdf-errors": "^0.0.0",
    "@awssolutions/cdf-express-middleware": "^0.0.0",
    "@awssolutions/cdf-lambda-invoke": "^0.0.0",
    "@awssolutions/cdf-logger": "^0.0.0",
    "@awssolutions/cdf-config-inject": "^0.0.0",
    "@awssolutions/cdf-provisioning-client": "^0.0.0",
    "@awssolutions/cdf-thing-list-builder": "^0.0.0",
    "@awssolutions/cdf-event-publisher": "~0.0.0",
    "adm-zip": "0.5.9",
    "aws-lambda": "1.0.6",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "dotenv-flow": "3.2.0",
    "express": "4.18.2",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.4.3",
    "inversify": "6.0.1",
    "js-yaml": "4.1.0",
    "just-clone": "3.2.1",
    "logform": "2.4.2",
    "ow": "0.23.0",
    "p-limit": "3.1.0",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "serverless-http": "2.7.0",
    "shortid": "2.2.16",
    "winston-transport": "4.4.0",
    "winston": "3.3.3",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@awssolutions/cdf-lambda-zip": "^0.0.0",
    "@rushstack/eslint-config": "2.3.4",
    "@types/adm-zip": "0.4.34",
    "@types/aws-lambda": "8.10.84",
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.10",
    "@types/dotenv-flow": "3.2.0",
    "@types/express": "4.17.16",
    "@types/jest": "26.0.23",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "7.26.0",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock-console": "1.0.1",
    "jest-mock-extended": "1.0.14",
    "jest-mock": "26.6.2",
    "jest-resolve": "26.6.2",
    "jest": "26.6.3",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
