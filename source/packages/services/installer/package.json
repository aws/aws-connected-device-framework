{
  "name": "@awssolutions/cdf-installer",
  "version": "0.0.0",
  "description": "CDF install wizard.",
  "author": "Dean Hart",
  "scripts": {
    "clean": "rushx clean:postrelease && npx shx rm -f bundle.zip",
    "clean:postrelease": "npx shx rm -rf dist deploy tsconfig.tsbuildinfo .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b",
    "create:installer": "npx tsc -b && npx shx chmod +x dist/index.js && ../../../common/temp/pnpm-local/node_modules/.bin/pnpm link --global",
    "test": "rushx lint && NODE_ENV= jest --silent --maxWorkers=$JEST_MAX_WORKERS"
  },
  "bin": {
    "cdf-cli": "./dist/index.js"
  },
  "dependencies": {
    "@awssolutions/cdf-config-inject": "workspace:^2.3.0",
    "@aws-sdk/client-cloudformation": "~3.53.0",
    "@aws-sdk/client-iam": "~3.53.0",
    "@aws-sdk/client-iot": "~3.53.0",
    "@aws-sdk/client-kms": "~3.53.0",
    "@aws-sdk/client-lambda": "~3.53.0",
    "@aws-sdk/client-ec2": "~3.53.0",
    "@aws-sdk/client-rds": "~3.53.0",
    "@aws-sdk/client-pricing": "~3.53.0",
    "@aws-sdk/client-s3": "~3.53.0",
    "@aws-sdk/client-sts": "~3.53.0",
    "@aws-sdk/client-ssm": "~3.53.0",
    "@aws-sdk/lib-storage": "~3.53.0",
    "chalk": "~4.1.2",
    "commander": "~9.2.0",
    "execa": "~5.1.1",
    "inquirer-fuzzy-path": "~2.3.0",
    "inquirer-s3": "~1.0.1",
    "inquirer": "8.2.1",
    "js-yaml": "4.1.0",
    "js-yaml-cloudformation-schema": "1.0.0",
    "node-localstorage": "~2.2.1",
    "ora": "~5.4.1",
    "recursive-copy": "~2.0.13",
    "listr2": "~3.13.5",
    "just-clone": "~5.0.1",
    "ow": "~0.28.1",
    "dotenv-flow": "~3.2.0",
    "find-up": "5.0.0"
  },
  "devDependencies": {
    "@awssolutions/eslint-config-custom": "workspace:~0.0.0",
    "@types/js-yaml": "4.0.5",
    "eslint": "8.46.0",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-mock": "26.6.2",
    "jest-resolve": "26.6.2",
    "jest": "26.6.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "@types/node": "^18.17.0",
    "@types/node-localstorage": "~1.3.0",
    "@types/inquirer": "8.2.1",
    "@types/inquirer-fuzzy-path": "~2.3.3",
    "@types/dotenv-flow": "~3.2.0"
  },
  "jest": {
    "globals": {},
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC",
  "private": true
}
