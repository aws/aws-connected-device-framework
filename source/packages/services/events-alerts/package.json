{
  "name": "@cdf/events-alerts",
  "version": "2.0.5",
  "description": "CDF Events (Alerts)",
  "author": "Dean Hart",
  "scripts": {
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b && npx shx cp -R 'src/config' 'dist/'",
    "test": "rushx lint && jest --silent",
    "start": "rushx build && APP_CONFIG_DIR=${PWD}/src/config nodemon dist/lambda_proxy.js",
    "bundle": "../../../infrastructure/bundle-package.bash events-alerts"
  },
  "dependencies": {
    "@cdf/errors": "^3.0.0",
    "@cdf/logger": "^3.0.0",
    "@cdf/config-inject": "^3.0.0",
    "@types/dot": "1.1.4",
    "amazon-dax-client": "1.2.6",
    "aws-sdk": "2.1048.0",
    "dot": "1.1.3",
    "inversify": "5.1.1",
    "inversify-binding-decorators": "4.0.0",
    "js-yaml": "4.1.0",
    "json-rules-engine": "5.3.0",
    "logform": "2.2.0",
    "ow": "0.23.0",
    "reflect-metadata": "0.1.13",
    "uuid": "8.3.2",
    "readable-stream": "3.6.0",
    "winston": "3.3.3",
    "winston-transport": "4.4.0",
    "dotenv-flow": "~3.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/uuid": "8.3.0",

    "@rushstack/eslint-config": "2.3.4",
    "eslint": "7.26.0",

    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "shx": "0.3.3",

    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-mock": "26.6.2",
    "jest-resolve": "26.6.2",
    "jest": "26.6.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "@types/dotenv-flow": "~3.2.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": true
}
