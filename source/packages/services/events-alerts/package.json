{
  "name": "@awssolutions/cdf-events-alerts",
  "version": "2.3.0",
  "description": "CDF Events (Alerts)",
  "author": "Dean Hart",
  "files": [
    "bundle.zip"
  ],
  "scripts": {
    "build": "tsc && npx shx cp -R 'src/config' 'dist/'",
    "bundle": "npm run bundle:build && npm run bundle:deploy",
    "bundle:build": "lambda-zip --entry-point src/lambda_proxy_ddbstream.ts --copy config:src/config:.env.defaults",
    "bundle:deploy": "node ../../../infrastructure/bundle-package.js",
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "start": "rushx build && APP_CONFIG_DIR=${PWD}/src/config nodemon dist/lambda_proxy.js",
    "test": "rushx lint && NODE_ENV= jest --silent --maxWorkers=$JEST_MAX_WORKERS"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@awssolutions/cdf-config-inject": "^2.3.0",
    "@awssolutions/cdf-errors": "^2.3.0",
    "@awssolutions/cdf-lambda-zip": "~0.0.0",
    "@awssolutions/cdf-logger": "^2.3.0",
    "@awssolutions/eslint-config-custom": "~0.0.0",
    "@awssolutions/simple-cdf-logger": "^2.3.0",
    "@types/cors": "2.8.10",
    "@types/dot": "1.1.4",
    "@types/dotenv-flow": "~3.2.0",
    "@types/jest": "26.0.23",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "amazon-dax-client": "1.2.7",
    "aws-sdk": "2.1386.0",
    "dot": "1.1.3",
    "dotenv": "^8.2.0",
    "dotenv-flow": "~3.2.0",
    "eslint": "8.46.0",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "js-yaml": "4.1.0",
    "json-rules-engine": "5.3.0",
    "logform": "2.4.2",
    "nodemon": "2.0.20",
    "nyc": "15.1.0",
    "ow": "0.23.0",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "winston-transport": "4.4.0"
  }
}
