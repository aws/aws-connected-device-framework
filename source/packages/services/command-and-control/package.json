{
  "name": "@awssolutions/cdf-command-and-control",
  "version": "2.5.2",
  "description": "CDF Command & Control",
  "author": "Dean Hart",
  "files": [
    "bundle.zip"
  ],
  "scripts": {
    "build": "tsc && npx shx cp -R 'src/config' 'dist/'",
    "bundle": "npm run bundle:build && npm run bundle:deploy",
    "bundle:build": "lambda-zip --entry-point src/lambda_*_handler.ts --copy config:src/config:.env.defaults",
    "bundle:deploy": "node ../../../infrastructure/bundle-package.js",
    "clean": "rushx clean:postrelease && npx shx rm -f bundle.zip",
    "clean:postrelease": "npx shx rm -rf dist deploy tsconfig.tsbuildinfo .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "start": "rushx build && APP_CONFIG_DIR=${PWD}/src/config node dist/lambda_rest_handler.js",
    "test": "rushx lint && NODE_ENV= jest --silent --maxWorkers=$JEST_MAX_WORKERS"
  },
  "config": {
    "environment": "development"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@awssolutions/cdf-assetlibrary-client": "workspace:^2.5.2",
    "@awssolutions/cdf-config-inject": "workspace:^2.5.2",
    "@awssolutions/cdf-errors": "workspace:^2.5.2",
    "@awssolutions/cdf-express-middleware": "workspace:~0.0.0",
    "@awssolutions/cdf-lambda-invoke": "workspace:^2.5.2",
    "@awssolutions/cdf-lambda-zip": "workspace:~0.0.0",
    "@awssolutions/cdf-logger": "workspace:^2.5.2",
    "@awssolutions/eslint-config-custom": "workspace:~0.0.0",
    "@awssolutions/simple-cdf-logger": "workspace:^2.5.2",
    "@awssolutions/cdf-provisioning-client": "workspace:^2.5.2",
    "@awssolutions/cdf-thing-list-builder": "workspace:~0.0.0",
    "@types/aws-lambda": "8.10.89",
    "@types/body-parser": "1.19.2",
    "@types/busboy": "0.2.3",
    "@types/cors": "2.8.10",
    "@types/dotenv-flow": "~3.2.0",
    "@types/express": "4.17.16",
    "@types/jest": "26.0.23",
    "@types/sinon": "9.0.9",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "ajv": "6.12.3",
    "aws-sdk": "2.1386.0",
    "aws-xray-sdk": "~3.3.4",
    "aws-xray-sdk-core": "~3.3.4",
    "body-parser": "1.20.1",
    "busboy": "0.3.1",
    "cors": "2.8.5",
    "deepmerge": "4.2.2",
    "dotenv-flow": "~3.2.0",
    "eslint": "8.46.0",
    "express": "4.19.2",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.4.3",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "js-yaml": "4.1.0",
    "logform": "2.4.2",
    "ow": "0.23.0",
    "p-limit": "3.1.0",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "serverless-http": "2.7.0",
    "short-unique-id": "4.4.4",
    "shx": "0.3.3",
    "sinon": "9.2.1",
    "superagent": "6.1.0",
    "supports-color": "8.0.0",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "winston": "3.3.3",
    "winston-transport": "4.4.0"
  }
}
