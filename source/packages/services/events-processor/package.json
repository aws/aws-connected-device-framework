{
  "name": "@aws-solutions/cdf-events-processor",
  "version": "0.0.2",
  "description": "CDF Events (Processor)",
  "author": "Dean Hart",
  "files": [
    "bundle.zip"
  ],
  "scripts": {
    "build": "npx tsc -b && npx shx cp -R 'src/config' 'dist/'",
    "bundle": "npm run bundle:build && npm run bundle:deploy",
    "bundle:build": "lambda-zip --entry-point src/lambda_*.ts --external file system --copy config:src/config:.env.defaults",
    "bundle:deploy": "node ../../../infrastructure/bundle-package.js",
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "start": "rushx build && APP_CONFIG_DIR=${PWD}/src/config nodemon dist/lambda_proxy_restapi.js",
    "test": "rushx lint && NODE_ENV= jest --silent --maxWorkers=$JEST_MAX_WORKERS"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@aws-solutions/cdf-config-inject": "^0.0.0",
    "@aws-solutions/cdf-errors": "^0.0.0",
    "@aws-solutions/cdf-express-middleware": "^0.0.0",
    "@aws-solutions/cdf-lambda-zip": "^0.0.0",
    "@aws-solutions/cdf-logger": "^0.0.0",
    "@rushstack/eslint-config": "2.3.4",
    "@types/aws-lambda": "8.10.65",
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.10",
    "@types/dotenv-flow": "~3.2.0",
    "@types/express": "4.17.16",
    "@types/express-version-request": "1.7.1",
    "@types/jest": "26.0.23",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "amazon-dax-client": "1.2.7",
    "aws-sdk": "2.1048.0",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-flow": "~3.2.0",
    "eslint": "7.26.0",
    "express": "4.18.2",
    "express-version-request": "1.7.0",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.4.3",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "js-yaml": "4.1.0",
    "json-rules-engine": "5.3.0",
    "logform": "2.4.2",
    "nodemon": "2.0.20",
    "nyc": "15.1.0",
    "ow": "0.23.0",
    "p-limit": "3.1.0",
    "readable-stream": "3.6.0",
    "reflect": "0.1.3",
    "reflect-metadata": "0.1.13",
    "serverless-http": "2.7.0",
    "shx": "0.3.3",
    "superagent": "7.1.1",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "winston-transport": "4.4.0"
  }
}
