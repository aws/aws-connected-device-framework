{
  "name": "@awssolutions/cdf-event-publisher",
  "version": "0.0.0",
  "description": "Utility library to be used by cdf components to publish events.",
  "author": "William Sia",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b",
    "test": "rushx lint && jest --silent --passWithNoTests"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "3.38.0",
    "@awssolutions/cdf-lambda-invoke": "^0.0.0",
    "@awssolutions/cdf-logger": "^0.0.0",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "logform": "2.4.2",
    "ow": "0.23.0",
    "superagent": "7.1.1",
    "winston": "3.3.3",
    "winston-transport": "4.4.0",
    "@aws-sdk/client-iot": "~3.37.0"
  },
  "devDependencies": {
    "@types/superagent": "4.1.15",

    "@rushstack/eslint-config": "2.3.4",
    "eslint": "7.26.0",

    "nyc": "15.1.0",
    "shx": "0.3.3",

    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-mock": "26.6.2",
    "jest-resolve": "26.6.2",
    "jest": "26.6.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "jest": {
    "globals": {
               },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC",
  "private": true
}
