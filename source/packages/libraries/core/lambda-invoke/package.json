{
  "name": "@awssolutions/cdf-lambda-invoke",
  "version": "0.0.2",
  "description": "Lambda Invoker Service",
  "license": "ISC",
  "author": "Hassan Khokhar",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc -b",
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "test": "rushx lint && jest --silent --passWithNoTests"
  },
  "jest": {
    "globals": {},
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "aws-sdk": "2.1048.0",
    "dotenv-flow": "~3.2.0",
    "http-errors": "1.8.0",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.4.3",
    "js-yaml": "4.1.0",
    "logform": "2.4.2",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "superagent": "7.1.1",
    "winston": "3.3.3",
    "winston-transport": "4.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "2.3.4",
    "@types/dotenv-flow": "~3.2.0",
    "@types/http-errors": "1.8.0",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "7.26.0",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  }
}
