{
  "name": "@awssolutions/cdf-client-request-signer",
  "description": "Make api gateway calls with IAM credentials",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "npx shx rm -rf dist *.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -p tsconfig.build.json",
    "test": "rushx lint && jest --silent --passWithNoTests --maxWorkers=$JEST_MAX_WORKERS"
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "3.252.0",
    "aws4": "1.12.0",
    "superagent": "7.1.1",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "2.3.4",
    "@types/aws4": "1.11.2",
    "@types/jest": "26.0.23",
    "@types/node": "^18.11.18",
    "@types/superagent": "4.1.16",
    "eslint": "7.26.0",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "jest": {
    "globals": {},
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC"
}
