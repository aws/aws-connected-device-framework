{
  "name": "@awssolutions/cdf-deployment-helper",
  "version": "0.0.0",
  "description": "CDF Deployment Helper",
  "author": "Hassan khokhar",
  "scripts": {
    "build": "npx tsc -p tsconfig.build.json && npx shx cp -R 'src/config' 'dist/config'",
    "bundle": "npm run bundle:build && npm run bundle:deploy",
    "bundle:build": "lambda-zip --entry-point src/lambda_*.ts --copy config:src/config:.env.defaults",
    "bundle:deploy": "node ../../../../infrastructure/bundle-package.js",
    "clean": "rushx clean:postrelease && npx shx rm -f bundle.zip",
    "clean:postrelease": "npx shx rm -rf dist deploy *.tsbuildinfo .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "test": "rushx lint && jest  --silent --passWithNoTests"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@awssolutions/cdf-commandandcontrol-client": "^0.0.0",
    "@awssolutions/cdf-config-inject": "^0.0.0",
    "@awssolutions/cdf-lambda-invoke": "^0.0.0",
    "@awssolutions/cdf-lambda-zip": "^0.0.0",
    "@awssolutions/cdf-logger": "^0.0.0",
    "@rushstack/eslint-config": "2.3.4",
    "@types/aws-lambda": "8.10.65",
    "@types/dotenv-flow": "~3.2.0",
    "@types/jest": "26.0.23",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "16.11.7",
    "@types/superagent": "4.1.15",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "aws-sdk": "2.1048.0",
    "cfn-response-promise": "1.0.1",
    "dotenv-flow": "~3.2.0",
    "eslint": "7.26.0",
    "http-errors": "1.8.0",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-express": "1.12.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "8.5.1",
    "logform": "2.4.2",
    "nodemon": "2.0.20",
    "nyc": "15.1.0",
    "ow": "0.23.0",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "shx": "0.3.3",
    "superagent": "7.1.1",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "winston": "3.3.3",
    "winston-transport": "4.4.0"
  }
}
