{
  "name": "@awssolutions/cdf-environment-sanitizer",
  "version": "2.3.0",
  "description": "Tools to sanitize process.env, for secure logging",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npx tsc -b",
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "test": "npm run lint && npx jest --maxWorkers=$JEST_MAX_WORKERS"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "2.3.4",
    "@types/archiver": "~5.3.1",
    "@types/glob": "~8.0.1",
    "@types/jest": "~29.4.0",
    "@types/node": "~18.11.18",
    "@types/yargs": "~17.0.20",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "7.26.0",
    "glob": "~8.1.0",
    "jest": "~29.4.1",
    "ts-jest": "~29.0.5",
    "typescript": "~4.9.4",
    "yargs": "~17.6.2"
  }
}
