{
  "name": "@awssolutions/cdf-config-inject",
  "version": "2.3.0",
  "description": "this library load config from files and inject it into process.env to make it accessible as environment variables",
  "author": "Atul Bargaje <atulbar@amazon.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "npx shx rm -rf dist deploy *.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -p tsconfig.build.json",
    "test": "rushx lint && APP_CONFIG_DIR='src/config' jest --silent --passWithNoTests --maxWorkers=$JEST_MAX_WORKERS"
  },
  "dependencies": {
    "@awssolutions/cdf-errors": "workspace:^2.3.0",
    "@awssolutions/cdf-logger": "workspace:^2.3.0",
    "@awssolutions/cdf-environment-sanitizer": "workspace:^2.3.0",
    "dotenv-flow": "~3.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@awssolutions/eslint-config-custom": "workspace:~0.0.0",
    "eslint": "8.46.0",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-mock": "26.6.2",
    "jest-resolve": "26.6.2",
    "jest": "26.6.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "@types/node": "^18.17.0",
    "@types/dotenv-flow": "~3.2.0"
  },
  "jest": {
    "globals": {},
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC"
}
