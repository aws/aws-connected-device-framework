{
  "name": "@awssolutions/cdf-greengrass2-provisioning-client",
  "version": "2.2.0",
  "description": "CDF Greengrass V2 Provisioning Client",
  "author": "Dean Hart",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "tsc",
    "test": "rushx lint && jest --silent --passWithNoTests --maxWorkers=$JEST_MAX_WORKERS"
  },
  "dependencies": {
    "@awssolutions/cdf-assetlibrary-client": "^2.2.0",
    "@awssolutions/cdf-lambda-invoke": "^2.2.0",
    "@awssolutions/cdf-client-request-signer": "^2.2.0",
    "@awssolutions/simple-cdf-logger": "^2.2.0",
    "aws-sdk": "2.1386.0",
    "http-errors": "~2.0.0",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.4.3",
    "js-yaml": "4.1.0",
    "logform": "2.4.2",
    "ow": "0.23.0",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "superagent": "7.1.1",
    "winston": "3.3.3",
    "winston-transport": "4.4.0",
    "dotenv-flow": "~3.2.0"
  },
  "devDependencies": {
    "@types/http-errors": "~2.0.1",
    "@types/node": "16.11.7",
    "@types/superagent": "4.1.15",
    "@rushstack/eslint-config": "2.3.4",
    "eslint": "7.26.0",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-mock": "26.6.2",
    "jest-resolve": "26.6.2",
    "jest": "26.6.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "@types/dotenv-flow": "~3.2.0"
  },
  "jest": {
    "globals": {},
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC"
}
