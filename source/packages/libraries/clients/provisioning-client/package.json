{
  "name": "@awssolutions/cdf-provisioning-client",
  "version": "2.4.0",
  "description": "Provisioning Library Client",
  "license": "ISC",
  "author": "Rahul Sareen",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "tsc",
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "test": "rushx lint && jest --silent --passWithNoTests --maxWorkers=$JEST_MAX_WORKERS"
  },
  "jest": {
    "globals": {},
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "@awssolutions/cdf-client-request-signer": "workspace:^2.4.0",
    "@awssolutions/cdf-lambda-invoke": "workspace:^2.4.0",
    "@awssolutions/simple-cdf-logger": "workspace:^2.4.0",
    "aws-sdk": "2.1386.0",
    "dotenv-flow": "~3.2.0",
    "http-errors": "~2.0.0",
    "inversify": "6.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.4.3",
    "js-yaml": "4.1.0",
    "logform": "2.2.0",
    "ow": "0.23.0",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "superagent": "7.1.1",
    "winston": "3.3.3",
    "winston-transport": "4.4.0"
  },
  "devDependencies": {
    "@awssolutions/eslint-config-custom": "workspace:~0.0.0",
    "@types/dotenv-flow": "~3.2.0",
    "@types/http-errors": "~2.0.1",
    "@types/jest": "26.0.23",
    "@types/superagent": "4.1.15",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "eslint": "8.46.0",
    "jest": "26.6.3",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "26.6.2",
    "jest-mock": "26.6.2",
    "jest-mock-extended": "1.0.14",
    "jest-resolve": "26.6.2",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  }
}
