{
  "name": "@awssolutions/cdf-integration-tests",
  "version": "2.6.0",
  "description": "CDF Integration Tests",
  "author": "Dean Hart",
  "scripts": {
    "clean": "rushx clean:postrelease",
    "clean:postrelease": "npx shx rm -rf dist build tsconfig.tsbuildinfo .nyc_output coverage dist build features/dist",
    "lint": "npx eslint . --ext '.ts'",
    "build": "rushx clean && npx tsc && npx shx mkdir -p features/dist && npx shx cp -R 'dist/step_definitions' 'features/dist/step_definitions' && npx shx cp -R 'src/testResources' 'dist/testResources' && npx shx cp -R 'dist/support' 'features/dist/support' && npx shx cp -R 'dist/di' 'features/dist/di' && npx shx cp -R 'src/config' 'features/dist/config'",
    "integration-test": "APP_CONFIG_DIR=${PWD}/src/config cucumber-js"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "~3.575.0",
    "@aws-sdk/client-greengrassv2": "3.575.0",
    "@aws-sdk/client-s3": "3.575.0",
    "@aws-sdk/s3-request-presigner": "~3.575.0",
    "@aws-sdk/client-cloudformation": "~3.575.0",
    "@awssolutions/cdf-assetlibrary-client": "workspace:^2.6.0",
    "@awssolutions/cdf-assetlibraryhistory-client": "workspace:^2.6.0",
    "@awssolutions/cdf-commandandcontrol-client": "workspace:^2.6.0",
    "@awssolutions/cdf-commands-client": "workspace:^2.6.0",
    "@awssolutions/cdf-config-inject": "workspace:^2.6.0",
    "@awssolutions/cdf-device-patcher-client": "workspace:^2.6.0",
    "@awssolutions/cdf-errors": "workspace:^2.6.0",
    "@awssolutions/cdf-greengrass2-provisioning-client": "workspace:^2.6.0",
    "@awssolutions/cdf-lambda-invoke": "workspace:^2.6.0",
    "@awssolutions/cdf-organizationmanager-client": "workspace:^2.6.0",
    "@awssolutions/cdf-logger": "workspace:^2.6.0",
    "@awssolutions/cdf-notifications-client": "workspace:^2.6.0",
    "@awssolutions/cdf-provisioning-client": "workspace:^2.6.0",
    "@awssolutions/simple-cdf-logger": "workspace:^2.6.0",
    "@cucumber/cucumber": "7.3.2",
    "atob": "2.1.2",
    "aws-iot-device-sdk-v2": "1.19.5",
    "aws-sdk": "2.1386.0",
    "axios": "1.6.0",
    "chai-string": "1.5.0",
    "chai-uuid": "1.0.6",
    "chai": "4.2.0",
    "dotenv-flow": "3.2.0",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.4.6",
    "inversify": "6.0.1",
    "js-yaml": "4.1.0",
    "json-stable-stringify": "1.0.1",
    "json-to-pretty-yaml": "1.2.2",
    "jsonpath-plus": "5.0.7",
    "jsonwebtoken": "9.0.0",
    "jszip": "3.10.1",
    "logform": "2.4.2",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "replace-in-file": "~6.3.2",
    "superagent": "7.1.1",
    "typed-rest-client": "1.8.9",
    "winston-transport": "4.4.0",
    "winston": "3.3.3",
    "yaml": "1.10.2",
    "deep-equal-in-any-order": "2.0.0"
  },
  "devDependencies": {
    "@awssolutions/eslint-config-custom": "workspace:~0.0.0",
    "@types/atob": "2.1.2",
    "@types/chai-string": "1.4.5",
    "@types/chai-uuid": "^1.0.2",
    "@types/chai": "4.2.14",
    "@types/dotenv-flow": "3.2.0",
    "@types/js-yaml": "3.12.5",
    "@types/json-stable-stringify": "1.0.32",
    "@types/jsonpath-plus": "5.0.1",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "^18.17.0",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "eslint": "8.46.0",
    "shx": "0.3.3",
    "typescript": "4.2.4",
    "@types/deep-equal-in-any-order": "1.0.1"
  }
}
