{
  "name": "@cdf/integration-tests",
  "version": "3.0.1",
  "description": "CDF Integration Tests",
  "author": "Dean Hart",
  "scripts": {
    "clean": "rushx clean:postrelease",
    "clean:postrelease": "npx shx rm -rf dist build tsconfig.tsbuildinfo .nyc_output coverage dist build features/dist",
    "lint": "npx eslint . --ext '.ts'",
    "build": "rushx clean && npx tsc && npx shx mkdir -p features/dist && npx shx cp -R 'dist/step_definitions' 'features/dist/step_definitions' && npx shx cp -R 'src/testResources' 'dist/testResources' && npx shx cp -R 'dist/support' 'features/dist/support' && npx shx cp -R 'dist/di' 'features/dist/di' && npx shx cp -R 'src/config' 'features/dist/config'",
    "integration-test": "APP_CONFIG_DIR=${PWD}/src/config cucumber-js"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "~3.38.0",
    "@aws-sdk/client-greengrassv2": "~3.38.0",
    "@aws-sdk/client-s3": "~3.38.0",
    "@aws-sdk/s3-request-presigner": "~3.38.0",
    "@cdf/assetlibrary-client": "^5.0.0",
    "@cdf/assetlibraryhistory-client": "^5.0.0",
    "@cdf/commands-client": "^5.0.0",
    "@cdf/errors": "^4.0.0",
    "@cdf/device-patcher-client": "^1.1.1",
    "@cdf/config-inject": "^4.0.0",
    "@cdf/greengrass2-provisioning-client": "^1.0.1",
    "@cdf/lambda-invoke": "^3.0.0",
    "@cdf/logger": "^4.0.0",
    "@cdf/notifications-client": "^4.0.0",
    "@cdf/provisioning-client": "^5.0.0",
    "atob": "~2.1.2",
    "aws-iot-device-sdk": "2.2.8",
    "aws-sdk": "2.952.0",
    "chai-string": "1.5.0",
    "chai-uuid": "1.0.6",
    "chai": "4.2.0",
    "cucumber": "6.0.5",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "inversify": "5.1.1",
    "js-yaml": "4.1.0",
    "json-stable-stringify": "1.0.1",
    "json-to-pretty-yaml": "~1.2.2",
    "jsonpath-plus": "~5.0.7",
    "jsonwebtoken": "8.5.1",
    "logform": "2.2.0",
    "readable-stream": "3.6.0",
    "reflect-metadata": "0.1.13",
    "replace-in-file": "~6.3.2",
    "superagent": "6.1.0",
    "typed-rest-client": "1.8.0",
    "winston-transport": "4.4.0",
    "winston": "3.3.3",
    "yaml": "~1.10.2",
    "dotenv-flow": "~3.2.0"
  },
  "devDependencies": {
    "@types/atob": "~2.1.2",
    "@types/aws-iot-device-sdk": "2.2.2",
    "@types/chai-string": "1.4.2",
    "@types/chai-uuid": "^1.0.2",
    "@types/chai": "4.2.14",
    "@types/cucumber": "6.0.1",
    "@types/js-yaml": "3.12.5",
    "@types/json-stable-stringify": "1.0.32",
    "@types/jsonpath-plus": "~5.0.1",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.6.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "7.26.0",
    "shx": "0.3.3",
    "typescript": "4.2.4",
    "@types/dotenv-flow": "~3.2.0"
  },
  "private": true
}
