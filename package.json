{
  "name": "cdf",
  "author": "Dean Hart <deanhart@amazon.com>",
  "scripts": {
    "reset": "pnpm m i --force && pnpm m run clean && pnpm m run reset && pnpm m i --force",
    "commit": "npx git-cz"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
